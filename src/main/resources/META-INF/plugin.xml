<idea-plugin>
    <id>org.zhangwenqing.jetbrains.idea-run-wdio</id>
    <name>WebdriverIO</name>
    <vendor email="me@zhangwenqing.org" url="http://www.zhangwenqing.org">Zhang Wenqing</vendor>
    <version>2021.2.22-0</version>
    <category>Tools Integration</category>

    <!-- Product and plugin compatibility requirements -->
    <!-- https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html -->
    <depends>com.intellij.modules.platform</depends>
    <depends>JavaScript</depends>
    <depends>NodeJS</depends>

    <extensions defaultExtensionNs="com.intellij">
        <configurationType implementation="org.zhangwenqing.jetbrains.execution.WdioConfigurationType"/>
        <programRunner implementation="org.zhangwenqing.jetbrains.execution.WdioRunProgramRunner"/>
        <runConfigurationProducer implementation="org.zhangwenqing.jetbrains.execution.WdioRunConfigurationProducer"/>
    </extensions>

</idea-plugin>
